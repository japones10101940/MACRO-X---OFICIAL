	Outros:CriarBotao("Abrir Porta (Casino)", function()

		local function code()
		local code = ""
		for i,v in pairs(game:GetService("Workspace").Casino.RobberyDoor.Codes:GetDescendants()) do
			if v:IsA("TextLabel") and v.Text ~= "" then
				code = code .. v.Text
			end
		end
		return code
		end


		game:GetService("Workspace").Casino.RobberyDoor.Keypad.Pad.CasinoKeypadSubmit:FireServer(code())

	end)
	Outros:CriarBotao("E (SEM DELAY)", function(value)

		local func;
		for i,v in pairs(getgc()) do
			if type(v) == "function" and debug.getinfo(v).name == "UpdateCircleActions" then
				func = v
			end
		end
		local goalTable = getupvalues(func)[2]
		for i,v in pairs(goalTable.CircleAction.Specs) do
			local old = v.Duration
			v.Duration = 0
		end

	end) 
