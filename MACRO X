local function SFWDG_fake_script() -- MENU.MOVIMENTAÇÃO 
	local script = Instance.new('LocalScript', MENU)

	local Menu = script.Parent;
	
	Menu.Active = true
	Menu.Draggable = true
end
coroutine.wrap(SFWDG_fake_script)()
