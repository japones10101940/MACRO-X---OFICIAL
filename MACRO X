			local UICorner_4 = Instance.new("UICorner")
			local Check2 = Instance.new("ImageLabel")
	
	
			liga.Name = "Item"
			liga.Parent = MENU2
			liga.BackgroundColor3 = Color3.fromRGB(16, 15, 16)
			liga.BorderSizePixel = 0
			liga.Position = UDim2.new(8.71804523, 0, 1.47317064, 0)
			liga.Size = UDim2.new(0, 260, 0, 22)
			liga.ZIndex = 3
			liga.Font = Enum.Font.FredokaOne
