local function WDUGH_fake_script() -- SCRIPT.MACRO X INICIO 
	local script = Instance.new('LocalScript', SCRIPT)

	local Texto = "MACRO X"
	local TextLabel = script.Parent["INICIO MACRO X"]
	
	
	
	for i = 1,#Texto do
		TextLabel.Text = string.sub(Texto, 1, i)
		wait(0.25)
	end
	wait(1)
	local Texto = "CARREGANDO SCRIPT..."
	local TextLabel = script.Parent["INICIO MACRO X"]
	for i = 1,#Texto do
		TextLabel.Text = string.sub(Texto, 1, i)
		wait(0.10)
	end
	local tempos = {}
	for i = 3,5,0.1 do
		tempos[#tempos + 1] = i
	end
	
	wait(1,tempos[math.random(1,#tempos)])
	for i = #Texto,1,-1 do
		TextLabel.Text = string.sub(Texto, 1, i)
		wait(0.01)
	end
	TextLabel.Text = ''
	_G.ScriptLoaded = true
	script.Parent.MENU.Visible = true
	script.Parent.MENU:TweenSize(UDim2.new(0, 389,0, 234), "InOut","Quart", 1, true)
end
coroutine.wrap(WDUGH_fake_script)()

