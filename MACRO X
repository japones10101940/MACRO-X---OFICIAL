			Check2.Position = UDim2.new(0.867298603, 0, 0.318181843, 0)
			Check2.Size = UDim2.new(0, 10, 0, 10)
			Check2.Visible = false
			Check2.ZIndex = 10
			Check2.Image = "http://www.roblox.com/asset/?id=5228569533"
			
			local Toggled = false
			
			
			liga.MouseButton1Click:Connect(function()
				Toggled = not Toggled
				
				Check2.Visible = Toggled
				
				
				pcall(callback, Toggled)
			end)
			
			-- Gui to Lua
			-- Version: 3.2
	
			-- Instances:
	
			local DropShadow = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local DropShadow_2 = Instance.new("Frame")
			local UICorner_2 = Instance.new("UICorner")
			local DropShadow_3 = Instance.new("Frame")
			local UICorner_3 = Instance.new("UICorner")
			local DropShadow_4 = Instance.new("Frame")
			local UICorner_4 = Instance.new("UICorner")
	
			--Properties:
	
			DropShadow.Name = "DropShadow"
			DropShadow.Parent = liga
			DropShadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropShadow.BorderSizePixel = 0
			DropShadow.Position = UDim2.new(0, -1, 0, -1)
			DropShadow.Size = UDim2.new(1, 0, 1, 0)
	
			UICorner.Parent = DropShadow
	
			DropShadow_2.Name = "DropShadow"
			DropShadow_2.Parent = liga
			DropShadow_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropShadow_2.BorderSizePixel = 0
			DropShadow_2.Position = UDim2.new(0, 1, 0, 1)
			DropShadow_2.Size = UDim2.new(1, 0, 1, 0)
	
			UICorner_2.Parent = DropShadow_2
	
			DropShadow_3.Name = "DropShadow"
			DropShadow_3.Parent = liga
			DropShadow_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropShadow_3.BorderSizePixel = 0
			DropShadow_3.Position = UDim2.new(0, 1, 0, -1)
			DropShadow_3.Size = UDim2.new(1, 0, 1, 0)
	
