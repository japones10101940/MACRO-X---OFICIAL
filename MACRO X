coroutine.wrap(PKVKS_fake_script)()
local function LLYDD_fake_script() -- MENU.TEXTO ANIMADO 
	local script = Instance.new('LocalScript', MENU)

	local Texto = "MACRO X"
	local TextLabel = script.Parent["MACRO X"]
	
	spawn(function()
		while true do
			for i = 1,#Texto do
				TextLabel.Text = string.sub(Texto, 1, i)
				wait(0.1)
			end
			wait(1)
			TextLabel.Text = "MACRO X"
		end
	end)
end
coroutine.wrap(LLYDD_fake_script)()
local function QOUJ_fake_script() -- MENU.NOTIFICACÃO MACRO 
	local script = Instance.new('LocalScript', MENU)

	repeat wait() until _G.ScriptLoaded
	wait(1)
	game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "MACRO X"; 
		Text = "Script carregado!"; 
		Duration = 6; 
		Icon = "rbxassetid://11092615099"; 
	})
end
coroutine.wrap(QOUJ_fake_script)()
