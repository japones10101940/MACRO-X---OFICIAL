local start = tick()

loadstring(game:HttpGet"https://github.com/webhooking2022/functions-library/raw/main/.functions")()
repeat wait() until functions

_G.ScriptLoaded = nil
local speaker = game.Players.LocalPlayer

for i,v in pairs(game.CoreGui:GetChildren()) do
    if v.Name == "SCRIPT" then
        v:Destroy()
    end
end
function FindTool(name, plr)
    plr = plr or speaker
    local tool 
    pcall(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v.Name == name then
                tool = v 
            end
        end
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA("Tool") and v.Name == name then
                tool = v 
            end
        end
    end)
    return tool
end

