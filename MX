		function Tab:CriarLabel(text, underline)
            underline = underline or false
            local TextButton = Instance.new("TextLabel")
            local UICorner = Instance.new("UICorner")
            local line1 = Instance.new("Frame")
            local line2 = Instance.new("Frame")
            
            
            TextButton.Name = "Item"
            TextButton.Parent = MENU2
            TextButton.BackgroundColor3 = Color3.fromRGB(16, 15, 16)
            TextButton.BorderSizePixel = 0
            TextButton.Size = UDim2.new(0, 260, 0, 22)
            TextButton.ZIndex = 4
            TextButton.Font = Enum.Font.FredokaOne
            TextButton.Text = text
            TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.TextSize = 14.000
            TextButton.TextXAlignment = (underline == false and Enum.TextXAlignment.Left) or (underline == true and Enum.TextXAlignment.Center)
            UICorner.Parent = TextButton
            
            line1.Name = "line1"
            line1.Parent = TextButton
            line1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            line1.BorderSizePixel = 0
            line1.Position = UDim2.new(0.0269230772, 0, 0.454545468, 0)
            line1.Size = UDim2.new(0, 80, 0, 2)
            line1.ZIndex = 5
            line1.Visible = underline
            
            line2.Name = "line2"
            line2.Parent = TextButton
            line2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            line2.BorderSizePixel = 0
            line2.Position = UDim2.new(0.661538422, 0, 0.454545468, 0)
            line2.Size = UDim2.new(0, 80, 0, 2)
            line2.ZIndex = 5
            line2.Visible = underline

			local RETORNAR = {}
			local last = 0
            spawn(function()
                while true do
                    last=last+0.1
                    wait(0.1)
                end
            end)
			function RETORNAR:Refresh(newtext)
                if last > 5 then
                    last = 0
                    local Texto = newtext
                    local TextLabel = TextButton
        
                    spawn(function()
                        for i = 1,#Texto do
                            TextLabel.Text = string.sub(Texto, 1, i)
                            wait(0.01)
                        end
                    end)
                else
                    TextButton.Text = newtext
                end
			end
			
			return RETORNAR
		end
		function Tab:CriarToggle(text, callback)
			local liga = Instance.new("TextButton")
	
			local UICorner = Instance.new("UICorner")
	
			local UICorner_2 = Instance.new("UICorner")
			local UICorner_3 = Instance.new("UICorner")
			local Check1 = Instance.new("Frame")
			local UICorner_4 = Instance.new("UICorner")
			local Check2 = Instance.new("ImageLabel")
	
	
			liga.Name = "Item"
			liga.Parent = MENU2
			liga.BackgroundColor3 = Color3.fromRGB(16, 15, 16)
			liga.BorderSizePixel = 0
			liga.Position = UDim2.new(8.71804523, 0, 1.47317064, 0)
			liga.Size = UDim2.new(0, 260, 0, 22)
			liga.ZIndex = 3
			liga.Font = Enum.Font.FredokaOne
			liga.Text = "  "..text
			liga.TextColor3 = Color3.fromRGB(255, 255, 255)
			liga.TextSize = 14.000
			liga.TextXAlignment = Enum.TextXAlignment.Left
	
	
	
			UICorner_3.Parent = liga
	
			Check1.Name = "Check1"
			Check1.Parent = liga
			Check1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Check1.BorderSizePixel = 0
			Check1.Position = UDim2.new(0.867298603, 0, 0.318181813, 0)
			Check1.Size = UDim2.new(0, 10, 0, 10)
			Check1.ZIndex = 4
	
			UICorner_4.CornerRadius = UDim.new(0, 3)
			UICorner_4.Parent = Check1
	
			Check2.Name = "Check2"
			Check2.Parent = liga
			Check2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Check2.Position = UDim2.new(0.867298603, 0, 0.318181843, 0)
			Check2.Size = UDim2.new(0, 10, 0, 10)
			Check2.Visible = false
			Check2.ZIndex = 10
			Check2.Image = "http://www.roblox.com/asset/?id=5228569533"
			
			local Toggled = false
			
			
			liga.MouseButton1Click:Connect(function()
				Toggled = not Toggled
				
				Check2.Visible = Toggled
				
				
				pcall(callback, Toggled)
			end)
			
			local DropShadow = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local DropShadow_2 = Instance.new("Frame")
			local UICorner_2 = Instance.new("UICorner")
			local DropShadow_3 = Instance.new("Frame")
			local UICorner_3 = Instance.new("UICorner")
			local DropShadow_4 = Instance.new("Frame")
			local UICorner_4 = Instance.new("UICorner")
	
			DropShadow.Name = "DropShadow"
			DropShadow.Parent = liga
			DropShadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropShadow.BorderSizePixel = 0
			DropShadow.Position = UDim2.new(0, -1, 0, -1)
			DropShadow.Size = UDim2.new(1, 0, 1, 0)
	
			UICorner.Parent = DropShadow
	
			DropShadow_2.Name = "DropShadow"
			DropShadow_2.Parent = liga
			DropShadow_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropShadow_2.BorderSizePixel = 0
			DropShadow_2.Position = UDim2.new(0, 1, 0, 1)
			DropShadow_2.Size = UDim2.new(1, 0, 1, 0)
	
			UICorner_2.Parent = DropShadow_2
	
			DropShadow_3.Name = "DropShadow"
			DropShadow_3.Parent = liga
			DropShadow_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropShadow_3.BorderSizePixel = 0
			DropShadow_3.Position = UDim2.new(0, 1, 0, -1)
			DropShadow_3.Size = UDim2.new(1, 0, 1, 0)
	
			UICorner_3.Parent = DropShadow_3
	
			DropShadow_4.Name = "DropShadow"
			DropShadow_4.Parent = liga
			DropShadow_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropShadow_4.BorderSizePixel = 0
			DropShadow_4.Position = UDim2.new(0, -1, 0, 1)
			DropShadow_4.Size = UDim2.new(1, 0, 1, 0)
	
			UICorner_4.Parent = DropShadow_4
		end
		return Tab
	end
	
	getgenv().newtab = Tab
end
