local function WDUGH_fake_script() 
	local script = Instance.new('LocalScript', SCRIPT)

	local Texto = "VIPER X HUB"
	local TextLabel = script.Parent["INICIO VIPER X HUB "]
	
	for i = 1,#Texto do
		TextLabel.Text = string.sub(Texto, 1, i)
		wait(0.25)
	end
	wait(1)
	local Texto = "CARREGANDO SCRIPT..."
	local TextLabel = script.Parent["INICIO VIPER X HUB "]
	for i = 1,#Texto do
		TextLabel.Text = string.sub(Texto, 1, i)
		wait(0.10)
	end
	wait(math.random())
	for i = #Texto,1,-1 do
		TextLabel.Text = string.sub(Texto, 1, i)
		wait(0.005)
	end
	TextLabel.Text = ''
	_G.ScriptLoaded = true
	script.Parent.MENU.Visible = true
	script.Parent.MENU:TweenSize(UDim2.new(0, 389,0, 234), "InOut","Quart", 1, true)
end
coroutine.wrap(WDUGH_fake_script)()

local aberto = true
game:GetService("UserInputService").InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.F3 then
		if aberto then
			aberto = false
			MENU:TweenSize(UDim2.new(0,0,0,0), "Out", "Quart", 1, true)
		else
			aberto = true
			MENU:TweenSize(UDim2.new(0, 389,0, 234), "Out", "Quart", 1, true)
		end
	end
end)
function dropdown(title, tab, callback)
    local blur = Instance.new("BlurEffect", game.Lighting)
    blur.Size = 0
    game:GetService("TweenService"):Create(blur, TweenInfo.new(1, Enum.EasingStyle.Linear), {Size = 35}):Play()
    MENU:TweenSize(UDim2.new(0,0,0,0), "Out", "Quart", 1, true)
    local ScreenGui = Instance.new("ScreenGui")
    local Dropdown = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local DropShadow = Instance.new("Frame")
    local UICorner_2 = Instance.new("UICorner")
    local DropShadow_2 = Instance.new("Frame")
    local UICorner_3 = Instance.new("UICorner")
    local DropShadow_3 = Instance.new("Frame")
    local UICorner_4 = Instance.new("UICorner")
    local DropShadow_4 = Instance.new("Frame")
    local UICorner_5 = Instance.new("UICorner")
    local TextLabel = Instance.new("TextLabel")
    local UICorner_6 = Instance.new("UICorner")
    local ScrollingFrame = Instance.new("ScrollingFrame")
    
    local UIListLayout = Instance.new("UIListLayout")
    
    
    ScreenGui.Parent = game.CoreGui
    
    Dropdown.Name = "Dropdown"
    Dropdown.Parent = ScreenGui
    Dropdown.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
    Dropdown.BorderSizePixel = 0
    Dropdown.AnchorPoint = Vector2.new(0.5, 0.5)
    Dropdown.Position = UDim2.new(0.5, 0, 0.5, 0)
    Dropdown.Size = UDim2.new(0,0,0,0)
    Dropdown.ZIndex = 2
    Dropdown.ClipsDescendants = true
    Dropdown:TweenSize(UDim2.new(0.338834941, 0, 0.642857134, 0), "Out", "Quart", 1, true)
    delay(1, function()
        Dropdown.ClipsDescendants = false
    end)
    UICorner.Parent = Dropdown
    
    DropShadow.Name = "DropShadow"
    DropShadow.Parent = Dropdown
    DropShadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    DropShadow.BorderSizePixel = 0
    DropShadow.Position = UDim2.new(0.00286532962, 0, 0.00246913591, 0)
    DropShadow.Size = UDim2.new(1, 0, 1, 0)
    
    UICorner_2.Parent = DropShadow
    
    DropShadow_2.Name = "DropShadow"
    DropShadow_2.Parent = Dropdown
    DropShadow_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    DropShadow_2.BorderSizePixel = 0
    DropShadow_2.Position = UDim2.new(-0.00286532962, 0, -0.00246913591, 0)
    DropShadow_2.Size = UDim2.new(1, 0, 1, 0)
    
    UICorner_3.Parent = DropShadow_2
    
    DropShadow_3.Name = "DropShadow"
    DropShadow_3.Parent = Dropdown
    DropShadow_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    DropShadow_3.BorderSizePixel = 0
    DropShadow_3.Position = UDim2.new(-0.00286532962, 0, 0.00246913591, 0)
    DropShadow_3.Size = UDim2.new(1, 0, 1, 0)
    
    UICorner_4.Parent = DropShadow_3
    
    DropShadow_4.Name = "DropShadow"
    DropShadow_4.Parent = Dropdown
    DropShadow_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    DropShadow_4.BorderSizePixel = 0
    DropShadow_4.Position = UDim2.new(0.00286532962, 0, -0.00246913591, 0)
    DropShadow_4.Size = UDim2.new(1, 0, 1, 0)
    
    UICorner_5.Parent = DropShadow_4
    
    TextLabel.Parent = Dropdown
    TextLabel.BackgroundColor3 = Color3.fromRGB(20,20,20)
    TextLabel.BorderSizePixel = 0
    TextLabel.Position = UDim2.new(-0.00286532962, 0, 0, 0)
    TextLabel.Size = UDim2.new(1.00573063, 0, 0.123456791, 0)
    TextLabel.ZIndex = 2
    TextLabel.Font = Enum.Font.FredokaOne
    TextLabel.Text = title..':'
    TextLabel.BackgroundTransparency = 1

    TextLabel.TextColor3 = Color3.fromRGB(255,255,255)
    TextLabel.TextSize = 32.000
    TextLabel.TextWrapped = true
    
    UICorner_6.Parent = TextLabel
    
    ScrollingFrame.Parent = Dropdown
    ScrollingFrame.Active = true
    ScrollingFrame.BackgroundColor3 = Color3.fromRGB(20,20,20)
    ScrollingFrame.BorderSizePixel = 0
    ScrollingFrame.Position = UDim2.new(0.0429799445, 0, 0.167901233, 0)
    ScrollingFrame.Size = UDim2.new(0.914040089, 0, 0.792592585, 0)
    ScrollingFrame.ZIndex = 3
    ScrollingFrame.ScrollBarThickness = 3
    UIListLayout.Parent = ScrollingFrame
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 5)
    tab = tab or {}
    
    for i,v in pairs(tab) do
        local template = Instance.new("TextButton")
        template.Name = "template"
        template.Parent = ScrollingFrame
        template.Active = false
        template.BackgroundColor3 = Color3.fromRGB(25,25,25)
        template.BorderSizePixel = 0
        template.Size = UDim2.new(1, 0, 0.1, 0)
        template.Visible = true
        template.ZIndex = 4
        template.Font = Enum.Font.FredokaOne
        template.Text = tostring(v)
        template.TextColor3 = Color3.fromRGB(255,255,255)
        template.TextSize = 20.000
        Instance.new("UICorner", template)
        template.MouseButton1Down:Connect(function()
            Dropdown:TweenSize(UDim2.new(0,0,0,0), "Out", "Quart", 1, true)
            Dropdown.ClipsDescendants = true
            MENU:TweenSize(UDim2.new(0, 389,0, 234), "Out", "Quart", 1, true)
            game:GetService("TweenService"):Create(blur, TweenInfo.new(1), {Size = 0}):Play()
            game.Debris:AddItem(blur, 1)
            game.Debris:AddItem(ScreenGui, 1)
            pcall(callback, tostring(v))
        end)
    
        local function SRIKL_fake_script() 
            local script = Instance.new('LocalScript', template)
    
            script.Parent.MouseEnter:Connect(function()
                game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3= Color3.fromRGB(20,20,20)}):Play()
    
            end)
            script.Parent.MouseLeave:Connect(function()
                game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3= Color3.fromRGB(25,25,25)}):Play()
    
            end)
        end
        coroutine.wrap(SRIKL_fake_script)()
    end    
end
