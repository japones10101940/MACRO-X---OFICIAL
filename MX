local function LLYDD_fake_script() 
	local script = Instance.new('LocalScript', MENU)

	local Texto = "VIPER X HUB "
	local TextLabel = script.Parent["VIPER X HUB "]
	
	spawn(function()
		while true do
			for i = 1,#Texto do
				TextLabel.Text = string.sub(Texto, 1, i)
				wait(0.1)
			end
			wait(1)
			TextLabel.Text = "VIPER X HUB "
		end
	end)
end
coroutine.wrap(LLYDD_fake_script)()
