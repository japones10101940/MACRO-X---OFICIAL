    local Arrest_Abort = false
    tab_Teleports:CreateButton('Arrest', function()
        spawn(function()
            Arrest_Abort = false
            repeat
                for i, v in pairs(main.arrest()) do
                    v:Callback(true)
                end
                for i, v in pairs(require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs) do
                    if v.Name == 'Eject' then
                        v:Callback(true)
                    end
                end
                task.wait()
            until tostring(player_tp.Team) == 'Prisoner' or Arrest_Abort == true
        end)
        repeat
            main.Tp3(function()
                return getRoot(player_tp).Position
            end, true)
            if Arrest_Abort then
                break
            end
            KeyEvent(Enum.KeyCode.Space, true)
            KeyEvent(Enum.KeyCode.Space, false)
            main.equipar('Handcuffs', true)
            notificar('Arresting', 2, 5)
            local TIMED = 0
            repeat
                TIMED = TIMED + 0.5
                main.Tp1(getRoot(player_tp).Position + Vector3.new(0, 10, 0))
    
                task.wait()
            until tostring(player_tp.Team) == 'Prisoner' or TIMED > 3
            main.equipar('Handcuffs', false)
    
            wait()
        until tostring(player_tp.Team) == 'Prisoner' or Arrest_Abort
        if not Arrest_Abort then
            notificar('Sucess arrested ' .. player_tp.Name, 1, 5)
        end
    end)
    
