        120,
        function(bool)
            aimconfig.fov = bool
        end
    )

    game:GetService('RunService').RenderStepped:Connect(
        function()
            if aimconfig.aimbot == true and aimconfig.aimbot2 == true then
                workspace.CurrentCamera.CFrame =
