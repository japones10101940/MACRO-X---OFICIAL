if not game:IsLoaded() then
    local msg = Instance.new("Message")
    msg.Parent = game.CoreGui 
    msg.Text = "MACRO X is waiting for the game to load." 
    game.Loaded:wait()
    msg:Destroy()
end
for i,v in pairs(game.CoreGui:GetChildren()) do 
    if v.Name == "ScreenGui" then 
        v:Destroy()
    end 
end

local lib = {}
local Menu = script.Parent;
local NOTIFICACOES = Instance.new("ScreenGui")
function transformar(pos)
    return Vector3.new(pos.X, 500, pos.Z)
end
NOTIFICACOES.Parent = game.CoreGui
local inicial = UDim2.new(0.729397297, 0, 0.0177165568, 0)
local fora = UDim2.new(1.729397297, 0, 0.0177165568, 0)

function organizar()
    for i,v in pairs(NOTIFICACOES:GetChildren()) do 
        local num = 0.1277165568 * i
        game:GetService("TweenService"):Create(v, TweenInfo.new(0.5), {Position = UDim2.new(0.729397297, 0, num, 0)}):Play()
    end
end

function notificar(texto, numero, tempo)
    local Cor = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local Frame = Instance.new("Frame")
    local UICorner_2 = Instance.new("UICorner")
    local fechar = Instance.new("TextButton")
    local barra = Instance.new("ImageLabel")
    local SubTitle = Instance.new("TextLabel")
    local sucess = Instance.new("ImageLabel")
    
    
    Cor.Name = "Cor"
    Cor.Parent = NOTIFICACOES
    Cor.BackgroundColor3 = Color3.fromRGB(60, 255, 0)
    Cor.Position = fora
    Cor.Size = UDim2.new(0.152521521, 0, 0.122047246, 0)
    
    UICorner.Parent = Cor
    
    
    
    
    Frame.Parent = Cor
    Frame.AnchorPoint = Vector2.new(1, 0.5)
    Frame.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
    Frame.Position = UDim2.new(1.7532233, 0, 0.5, 0)
    Frame.Size = UDim2.new(1.71774197, 0, 1, 0)
    
    UICorner_2.Parent = Frame
    
    fechar.Name = "fechar"
    fechar.Parent = Frame
    fechar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    fechar.BackgroundTransparency = 1.000
    fechar.Size = UDim2.new(0.906103313, 0, 1, 0)
    fechar.Font = Enum.Font.SourceSans
    fechar.Text = ""
    fechar.TextColor3 = Color3.fromRGB(0, 0, 0)
    fechar.TextSize = 14.000
    
    barra.Name = "barra"
    barra.Parent = Frame
    barra.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
    barra.BackgroundTransparency = 1.000
    barra.Position = UDim2.new(0.963461876, 0, 0.14306581, 0)
    barra.Size = UDim2.new(0.00469483575, 0, 0.693548381, 0)
    barra.Image = "rbxassetid://3570695787"
    barra.ImageColor3 = Color3.fromRGB(255, 255, 0)
    barra.ScaleType = Enum.ScaleType.Slice
    barra.SliceCenter = Rect.new(100, 100, 100, 100)
    barra.SliceScale = 0.167
    
    SubTitle.Name = "SubTitle"
    SubTitle.Parent = Cor
    SubTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    SubTitle.BackgroundTransparency = 1.000
    SubTitle.Position = UDim2.new(0.21843763, 0, 0.0, 0)
    SubTitle.Size = UDim2.new(1.725806475, 0, 1.290322572, 0)
    SubTitle.Font = Enum.Font.FredokaOne
    SubTitle.Text = texto
    SubTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    SubTitle.TextSize = 14.000
    SubTitle.TextWrapped = true
    
    sucess.Name = "sucess"
    sucess.Parent = Cor
    sucess.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    sucess.BackgroundTransparency = 1.000
    sucess.Position = UDim2.new(0.0967741907, 0, 0.258064508, 0)
    sucess.Size = UDim2.new(0.25, 0, 0.483870953, 0)
    
    sucess.ImageColor3 = Color3.fromRGB(0, 255, 0)
    
    local notify_id = "rbxassetid://654933978"
    local warnn = "rbxassetid://12069916896"
    local sucesss = "rbxassetid://12348387864"
    local erro = "rbxassetid://12069914428"
    
    sucess.Image = (numero == 1 and sucesss) or (numero == 2 and warnn) or (numero ==3 and erro)
    sucess.ImageColor3 = (numero == 1 and Color3.fromRGB(0, 255, 255)) or Color3.fromRGB(255, 255, 255)
    Cor.BackgroundColor3 = (numero == 1 and Color3.fromRGB(0, 255, 0)) or (numero == 2 and Color3.fromRGB(255, 255, 100)) or (numero == 3 and Color3.fromRGB(255, 0, 0))
    
    game:GetService("TweenService"):Create(Cor, TweenInfo.new(0.8), {Position = inicial}):Play()
    organizar()
    local deletou = false
    fechar.MouseButton1Click:Connect(function()
        if deletou == false then 
            deletou = true 
            game:GetService("TweenService"):Create(Cor, TweenInfo.new(0.8), {Position = Cor.Position + UDim2.new(1, 0, 0, 0)}):Play()
            delay(0.8, function()
                Cor:Destroy()
                organizar()
                
            end)
        end
    end)
    local Sound = Instance.new("Sound", workspace)
    Sound.Volume = 1 
    Sound.SoundId = notify_id
    Sound:Play()
    game.Debris:AddItem(Sound, 5)
    spawn(function()
        game:GetService("TweenService"):Create(barra, TweenInfo.new(tempo, Enum.EasingStyle.Linear), {ImageColor3 = Color3.fromRGB(255, 0, 0)}):Play()
        delay(tempo, function()
            if deletou == false then 
                deletou = true 
                game:GetService("TweenService"):Create(Cor, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Position = Cor.Position + UDim2.new(1, 0, 0, 0)}):Play()
                delay(0.8, function()
                    Cor:Destroy()
                    organizar()
                    
                end)
            end
        end)
    end)
end


