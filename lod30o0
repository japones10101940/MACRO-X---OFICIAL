    local Old_Specs = {}
    
    tab_Others:CreateToggle('No E Delay', function(bool)
        if bool then
            local Teste = require(game:GetService('ReplicatedStorage').Module.UI)
            for i, a in next, Teste.CircleAction.Specs do
                Old_Specs[#Old_Specs + 1] = {a, a.Duration}
                a.Duration = 0
            end
        else
            local Teste = require(game:GetService('ReplicatedStorage').Module.UI)
            for i, a in next, Teste.CircleAction.Specs do
                for i, v in pairs(Old_Specs) do
                    if v == a then
                        a.Duration = v[2]
                    end
                end
            end
        end
    end)
    
