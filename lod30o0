    hookfunction(require(game:GetService('ReplicatedStorage').Module.AlexRagdoll).Ragdoll, function()
        return wait(9e9)
    end)


    notificar("MACRO X Loaded", 1, 5)

    local tab_Auto = moretti:CreateTab('Farm')
    
