    local notificar_roubos = false
    Outros:CriarToggle(
        'Notificar roubos',
        function(bool)
            notificar_roubos = bool
        end
    )
    local esperar_notificacao = false
    for i, v in pairs(Robberys) do
        spawn(
            function()
                while true do
                    local old = is_opened(v)
                    repeat
                        wait()
                    until is_opened(v) ~= old
                    if notificar_roubos and is_opened(v) ~= 'roubando' then
                        if esperar_notificacao == false then
                            esperar_notificacao = true
                            notificar(
                                v.."\n"..
                                ((is_opened(v) == 'aberto' and 'Open') or (is_opened(v) == 'fechado' and 'Close')),
                                2,4
                            )
                            wait(5)
                            esperar_notificacao = false
                        else
                            repeat
                                wait()
                            until esperar_notificacao == false
                            notificar(
                                v.."\n"..
                                ((is_opened(v) == 'aberto' and 'Open') or (is_opened(v) == 'fechado' and 'Close')),
                                2,4
                            )
                        end
                    end
                end
            end
        )
    end

