            if b[v.Name].Color ~= Color3.fromRGB(167, 75, 43) then 
                spawn(function()
                    repeat
                        repeat 
                            fireclickdetector(v.ClickDetector, 0)
                            wait(0.3)
                            fireclickdetector(v.ClickDetector, 1)
                            wait(0.3)
                        until b[v.Name].Color == Color3.fromRGB(167, 75, 43)
                        wait(0.5)
                        
                    until b[v.Name].Color == Color3.fromRGB(167, 75, 43)
                    
                end)
            end
        end
    end)

