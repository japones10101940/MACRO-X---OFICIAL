function sairjoa()
    repeat 
        for i = 1,40 do 
            task.wait()
            game.Players.LocalPlayer.Character:PivotTo(CFrame.new(1088.0675048828125, 122.03177642822266, 1358.5916748046875))
        end
        wait(0.5)
    until (Vector3.new(1072.07080078125, 101.7808837890625, 1249.11669921875) - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 200

end

