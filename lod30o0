    DropShadow_2.BorderSizePixel = 0
    DropShadow_2.Position = UDim2.new(-0.0031152647, 0, -0.00952380989, 0)
    DropShadow_2.Size = UDim2.new(1, 0, 1, 0)

    UICorner_3.CornerRadius = UDim.new(0, 25)
