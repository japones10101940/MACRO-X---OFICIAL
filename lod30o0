local function getRoot(plr)
    plr = plr or game.Players.LocalPlayer
    local sucess, root =
        pcall(
        function()
            return plr.Character.HumanoidRootPart
        end
    )

    if not sucess then
        return false
    end

    return root
end
function temmoney()
    local sucess, retorno =
        pcall(
        function()
            return string.find(
                game.Players.LocalPlayer.PlayerGui.NotificationGui.ContainerNotification.Message.Text,
                "I don't have money"
            )
        end
    )
    if not sucess then
        return false
    end
    if retorno == true then
        return true
    else
        return false
    end
end

function getCar()
    for i, v in pairs(game:GetService('Workspace').Vehicles:GetDescendants()) do
        if v.Name == 'Seat' and v:FindFirstChild('PlayerName') ~= nil then
            if v.PlayerName.Value == game.Players.LocalPlayer.Name then
                return v.Parent.Engine
            end
        end
    end
    return nil
end

local Robberys = {
    ['1'] = 'Bank',
    ['2'] = 'Jewelry Store',
    ['3'] = 'Museum',
    ['4'] = 'Power Plant',
    ['5'] = 'Passenger Train',
    ['6'] = 'Cargo Train',
    ['7'] = 'Cargo Ship',
    ['8'] = 'Cargo Plane',
    ['9'] = 'Gas Station',
    ['10'] = 'Donut Store',
    ['11'] = 'Money Truck',
    ['12'] = 'Home Vault',
    ['13'] = 'Tomb',
    ['14'] = 'Casino',
    ['15'] = 'Mansion'
}

function get_rob(nome)
    for i, v in pairs(Robberys) do
        if v:lower() == nome:lower() then
            return i
        end
    end
end

function is_opened(rob)
    local roubo = get_rob(rob)

    local State = game:GetService('ReplicatedStorage').RobberyState:FindFirstChild(roubo)

    return (tostring(State) == 'nil' and 'fechado') or
        ((State.Value == 1 and 'aberto') or (State.Value == 2 and 'roubando') or (State.Value == 3 and 'fechado'))
end
function bag()
    local title = game:GetService("Players").LocalPlayer.PlayerGui.RobberyMoneyGui.Container.Bottom.Progress.Amount

    local args = title.Text:split(" / ")

    if args[1] == args[2] then 
        return true 
    else
        return false 
    end
end
function teleport_path_find(pos_custom, howmuch)
    howmuch = howmuch or 5

    local Dummy = game.Players.LocalPlayer.Character

    local Humanoid = Dummy:FindFirstChild('Humanoid')

    local pathService = game:GetService('PathfindingService')

    local path = pathService:CreatePath()

    local waypoints, nextWaypoint

    local ReleaseConnection

    local function tpp(pos)
        local Finished = false
        local sucess =
            pcall(
            function()
                path:ComputeAsync(Dummy.HumanoidRootPart.Position, pos)
            end
        )

        if sucess then
            waypoints = path:GetWaypoints()
            if waypoints == 0 then
                return
            end

            local BodyPosition = Instance.new('BodyPosition')
            BodyPosition.D = 500
            BodyPosition.P = 15e4
            BodyPosition.MaxForce = Vector3.new(9e9, 9e9, 9e9)
            for i = 1, #waypoints do
                if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
                    if game.Players.LocalPlayer.Character.Humanoid.Sit == false then
                        BodyPosition.D = 11e2
                    else
                        BodyPosition.D = 5e2
                    end
                    BodyPosition.Parent = getRoot(speaker)
                    getRoot(speaker).Velocity = Vector3.new(0, 0, 0)
                    local pos_target = waypoints[i].Position + Vector3.new(0, howmuch, 0)
                    BodyPosition.Position = pos_target
                    repeat
                        task.wait()
                    until (getRoot(speaker).Position - pos_target).Magnitude < 0.2 or abort == true
                end
            end
            getRoot(speaker).Velocity = Vector3.new(0, 0, 0)

            BodyPosition:Destroy()
        end
    end

    tpp(pos_custom)
end
local loop
local already_deleted = true
function no_clip(bool)
    if bool and already_deleted then
        already_deleted = false
        loop =
            game:GetService('RunService').Stepped:Connect(
            function()
                for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if v:IsA('BasePart') then
                        v.CanCollide = false
                    end
                end
            end
        )
    else
        loop:Disconnect()
        already_deleted = true
    end
end





Notify = function(um, dois, tres)
    spawn(
        function()
            repeat
                wait()
            until game:GetService('Players').LocalPlayer.PlayerGui.NotificationGui ~= nil
            local sound = Instance.new('Sound', workspace)
            sound.SoundId = 'rbxassetid://700153902'
            sound:Play()
            for i, v in pairs(game.CoreGui:GetChildren()) do
                if v.Name == 'NotificationGui' then
                    v:Destroy()
                end
            end
            local menu = game:GetService('Players').LocalPlayer.PlayerGui.NotificationGui:Clone()

            menu.Parent = game.CoreGui
            menu.Enabled = true

            title = um .. '\n' .. dois
            spawn(
                function()
                    for i = 1, #title do
                        menu.ContainerNotification.Message.Text = string.sub(title, 1, i)
                        menu.Sound:Play()
                        wait(0.05)
                    end
                    wait(tres)
                    menu:Destroy()
                    sound:Destroy()
                end
            )
        end
    )
end

function esperar(bloco, numero, other)
    other = other or function()end
    local dist = 5000
    repeat
        if other then
            other()
        end
        local localplayer = game.Players.LocalPlayer
        local character = localplayer.Character or localplayer.CharacterAdded:wait()
        local bloco_position =
            (typeof(bloco) == 'Vector3' and bloco) or (typeof(bloco) == 'Instance' and bloco.Position)
        local root_position = character:FindFirstChild('HumanoidRootPart').Position

        dist = (bloco_position - root_position).Magnitude
        task.wait()
    until (dist <= numero) or game.Players.LocalPlayer.Character.Humanoid.Health == 0
end
local speaker = game.Players.LocalPlayer
local use_car = true

function getCar()
    for i, v in pairs(game:GetService('Workspace').Vehicles:GetDescendants()) do
        if v.Name == 'Seat' and v:FindFirstChild('PlayerName') ~= nil then
            if v.PlayerName.Value == game.Players.LocalPlayer.Name then
                return v.Parent.Engine
            end
        end
    end
    return nil
end
function getCamaro()
    local vehicle, spec, nearest = nil, nil, 1000
    local Root = getRoot(speaker)
    for i, v in next, require(game.ReplicatedStorage.Module.UI).CircleAction.Specs do
        if v.Name == 'Enter Driver' and v.ValidRoot.Name == 'Camaro' --[[or Jeep]] then
            local dist = (v.ValidRoot.PrimaryPart.Position - Root.Position).Magnitude

            if dist < nearest then
                vehicle, spec, nearest = v.ValidRoot, v, dist
                --[[if game.Players.LocalPlayer.Character.Humanoid.Sit == false then
                    for i, v in next, require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs do
                        if v.Name == 'Enter Driver' then
                            pcall(
                                function()
                                    v.Duration = 1
                                    v:Callback(true)
                                end
                            )
                        end
                    end
                end]]
            end
        end
    end
    if vehicle ~= nil then
        return vehicle, spec, nearest
    end
end

function instant(pos)
    if getCar() then
        getCar().CFrame = CFrame.new(pos)
    end
    if not getCar() then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
    end
end
local function getCharacter(plr)
    local char = plr.Character

    if char then
        return char
    end
    return false
end
local function getHead(plr)
    local char = plr.Character

    if char and char:FindFirstChild('Head') then
        return char.Head
    end
    return false
end
local function getHumanoid(plr)
    local char = plr.Character

    if char and char:FindFirstChildWhichIsA('Humanoid') then
        return char:FindFirstChildWhichIsA('Humanoid')
    end
    return false
end
local function FindTool(toolName, plr)
    plr = plr or game.Players.LocalPlayer

    if getCharacter(plr):FindFirstChild(toolName) then
        return getCharacter(plr):FindFirstChild(toolName)
    elseif plr.Backpack:FindFirstChild(toolName) then
        return plr.Backpack:FindFirstChild(toolName)
    end
    return false
end
local function FindPlayer(name)
    for i, v in pairs(game.Players:GetChildren()) do
        local name1 = string.sub(v.Name:lower(), 1, #name)
        local name2 = name:lower()
        local name3 = string.sub(v.DisplayName:lower(), 1, #name)

        if name1 == name2 or name3 == name2 then
            return v
        end
    end
    return nil
end
pode = true
function tp(vector, velo, velo2)
    velo2 = velo2 or 440
    if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
        return 
    end
    if pode == false then
        return
    end
    pode = false
    velo = velo or 140
    isup = false
    custom = nil
    custom = custom or nil
    isup = isup or false
    local partPos
    vector = vector or Vector3.new(0, 0, 0)
    local reached = false
    local abort = false
    local isplayer = false
    if tostring(typeof(vector):lower()) == 'string' then
        vector = FindPlayer(tostring(vector))
        if getRoot(vector) then
        else
            game:GetService('StarterGui'):SetCore(
                'SendNotification',
                {
                    Title = 'Error',
                    Text = 'Character of player is nil.',
                    Duration = 5
                }
            )
            return
        end

        if vector == nil then
            game:GetService('StarterGui'):SetCore(
                'SendNotification',
                {
                    Title = 'Error',
                    Text = 'Error to get player.',
                    Duration = 5
                }
            )
            return
        end

        vector = getRoot(vector)
    end
    if typeof(vector) == 'Instance' then
        isplayer = true
        partPos = vector
    end

    spawn(
        function()
            if isplayer then
                while reached == false and abort == false do
                    vector = partPos.Position
                    if isup then
                        vector = vector + Vector3.new(0, 400, 0)
                    end
                    wait()
                end
            end
        end
    )

    customvelo = customvelo or 5e4
    faltando = faltando or false
    isplayer = isplayer or false

    if customvelo == nil then
        customvelo = 150000
    end
    local aperta = false

    local item = require(game:GetService('ReplicatedStorage').Module.AlexRagdoll)
    local oldfunction = item.Ragdoll
    item.Ragdoll = function()
        return wait(9e9)
    end
    local BodyVelocity = Instance.new('BodyPosition')
    BodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    BodyVelocity.Position = getRoot(speaker).Position
    BodyVelocity.D = 500
    BodyVelocity.P = 50000
    BodyVelocity.Parent = getRoot(speaker)
    local old = teleport_velo
    local Loop =
        game:GetService('RunService').Stepped:Connect(
        function(_, step)
            local yourRoot = getRoot(speaker)
            local diff
            diff = (vector - getRoot(speaker).Position)
            if speaker.Character:FindFirstChild('Humanoid').Sit == true then
                BodyVelocity.P = customvelo
                teleport_velo = velo2
            else
                teleport_velo = velo
            end
            if (vector - getRoot(speaker).Position).Magnitude < 1 or speaker.Character.Humanoid.Health == 0 then
                reached = true
            end
            BodyVelocity.Position =
                getRoot(speaker).Position + diff.Unit * math.min(step * teleport_velo, diff.Magnitude)
            if getCar() then
                getCar().BodyGyro.MaxTorque = Vector3.new(9e9, 0, 9e9)
            end
        end
    )
    repeat
        task.wait()
        getRoot(speaker).Velocity = Vector3.new(0, 0, 0)
        getRoot(speaker).CFrame = CFrame.new(getRoot(speaker).Position)
        if getCar() then 
            for i,v in pairs(getCar().Parent:GetChildren()) do 
                if v:IsA("BasePart") then 
                    v.Velocity = Vector3.new(0,0,0)
                end
            end
        end
    until reached == true or abort == true or game.Players.LocalPlayer.Character.Humanoid.Health == 0
    getRoot(speaker).Velocity = Vector3.new(0, 0, 0)
    Loop:Disconnect()
    BodyVelocity:Destroy()
    if getCar() then
        getCar().BodyGyro.MaxTorque = Vector3.new(0, 0, 0)
    end
    getRoot(speaker).Velocity = Vector3.new(0, 3, 0)
    pode = true
    task.wait()
end

function Tp1(...)
    tp(...)
end
function tp1(...)
    Tp1(...)
end


function tpporcima(vector, procurar)
    procurar = procurar or false
    notificar("Teleportando...", 2, 3)
    if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then 
        return 
    end

    if getCar() then
        for i,v in pairs(getCar().Parent:GetChildren()) do 
            if v:IsA("BasePart") then 
                v.CFrame = CFrame.new(getRoot().Position + Vector3.new(0, 542, 0))
            end
        end 

    else
        instant(getRoot().Position + Vector3.new(0, 542, 0))
    end
    
    wait()
    tp(vector + Vector3.new(0, 542, 0))
    if getCar() then
        for i,v in pairs(getCar().Parent:GetChildren()) do 
            if v:IsA("BasePart") then 
                v.CFrame = CFrame.new(vector)
            end
        end
        
    else
        instant(vector + Vector3.new(0, 15, 0))
        wait(0.1)
        for i = 1, 5 do
            instant(vector + Vector3.new(0, 5, 0))

            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -1, 0)
            wait()
        end
    end
end


function Tp2(vector, procurar)
    procurar = procurar or false
    if procurar then 
-- Configurações
local alturaMaxima = 1000 -- altura máxima a ser verificada
local numeroDeVerificacoes = 1000 -- número de posições a serem verificadas


function verificarTeto(posicao)

    local humanoid = game.Players.LocalPlayer.Character.Humanoid
    local raioDeColisao = Vector3.new(2, 2, 2)
    local parteDeTeste = Instance.new("Part")
    parteDeTeste.Size = Vector3.new(0, 0, 0)
    parteDeTeste.Anchored = true
    parteDeTeste.CanCollide = true
    parteDeTeste.Position = posicao + Vector3.new(0, 10, 0) -- verificação a 10 unidades acima da posição
    parteDeTeste.Parent = workspace
    local parteTocada, pontoDeContato = workspace:FindPartOnRayWithIgnoreList(Ray.new(parteDeTeste.Position, Vector3.new(0, 1, 0).unit * alturaMaxima), {parteDeTeste})
    parteDeTeste:Destroy()
    if parteTocada then
        return true
    else
        return false
    end
end
end

    local vehicle, spec = getCamaro()
    if
        game.Players.LocalPlayer.Character.Humanoid.Sit == false and use_car and
            (vector - getRoot(speaker).Position).Magnitude > 100 and
            (vehicle and vehicle.Seat and
                (vehicle.Seat.Position - getRoot(speaker).Position).Magnitude <
                    (vector - getRoot(speaker).Position).Magnitude)
     then
        tpporcima(vehicle.Seat.Position)
        
        local esperando = 0
        repeat
            wait()
            esperando = esperando + 0.1
            spec:Callback(true)
        until getHumanoid(speaker).Sit == true or esperando > 3
        wait(0.5)
    end

    tpporcima(vector)
end
getgenv().Tp1 = Tp1
repeat
    wait()
until Tp1 ~= nil and Tp2 ~= nil

