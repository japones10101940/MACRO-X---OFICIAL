function robrosca()
    Tp2(Vector3.new(93.49116516113281, 18.863420486450195, -1491.834228515625), true)
    saircarro()

    wait(0.4)
    Tp1(Vector3.new(97.2082748413086, 19.517820358276367, -1530.1361083984375))
    no_clip(true)
    Tp1(Vector3.new(96.95466613769531, 19.517820358276367, -1528.409912109375))
    wait()
    no_clip(false)
    Tp1(Vector3.new(113.69667053222656, 19.717845916748047, -1578.6995849609375))
    wait()
    Tp1(Vector3.new(76.89389038085938, 19.717849731445312, -1589.548828125))
    local continuar = false
    for i, v in next, require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs do
        if v.Name == 'Rob' then
            spawn(
                function()
                    repeat
                        for i, v in next, require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs do
                            if v.Name == 'Rob' then
                                v:Callback(true)
                            end
                        end
                        wait(1)
                    until is_opened('donut store') == 'fechado' or temmoney() == true
                    continuar = true
                end
            )
        end
    end
    repeat
        wait()
    until continuar
    Tp1(Vector3.new(76.89389038085938, 19.717849731445312, -1589.548828125))
    Tp1(Vector3.new(113.69667053222656, 19.717845916748047, -1578.6995849609375))
    Tp1(Vector3.new(96.95466613769531, 19.517820358276367, -1528.409912109375))

    no_clip(true)
    Tp1(Vector3.new(93.41072845458984, 18.95391082763672, -1512.1981201171875))
    no_clip(false)
end

function Tp(Vector)
    Tp1(Vector)
end

function saircarro()
    if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
        repeat
            game:GetService('VirtualInputManager'):SendKeyEvent(
                true,
                Enum.KeyCode.Space,
                false,
                game
            )
            wait(0.1)
        until game.Players.LocalPlayer.Character.Humanoid.Sit == false
        game:GetService('VirtualInputManager'):SendKeyEvent(false, Enum.KeyCode.Space, false, game)
    end
end

function notificacaonormal(um, dois, tres)
    game:GetService('StarterGui'):SetCore(
        'SendNotification',
        {
            Title = um,
            Text = dois,
            Duration = tres
        }
    )
end
function notificacao(um, dois, tres)
    Notify(um, dois, tres)
end


function laser1()
    pcall(function()

        for i, v in pairs(workspace:GetDescendants()) do
            if v.Name == 'BarbedWire' or v.Name == 'Shadow' or v.Name ==
                'Lasers' or v.Name == 'LasersMoving' or v.Name == 'Shadow' or
                v.Name == 'Light' or
                (v:IsA('Part') and tostring(v.BrickColor) == 'Dusty Rose') or
                v.Name == 'LaserCarousel' or
                (v.Name == 'InnerModel' and v.Parent.Name == 'Camera') then
                local oldParent = v.Parent
                v.Parent = game:GetService('ReplicatedStorage')
            end
        end
        workspace.DescendantsAdded:Connect(function(v)
            if v.Name == 'BarbedWire' or v.Name == 'Shadow' or v.Name ==
            'Lasers' or v.Name == 'LasersMoving' or v.Name == 'Shadow' or
            v.Name == 'Light' or
            (v:IsA('Part') and tostring(v.BrickColor) == 'Dusty Rose') or
            v.Name == 'LaserCarousel' or
            (v.Name == 'InnerModel' and v.Parent.Name == 'Camera') then
            local oldParent = v.Parent
            v.Parent = game:GetService('ReplicatedStorage')
            end
        end)
    end)
end
