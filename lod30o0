        local vehicle,spec = main.getCamaro()
        if vehicle then 
            spec:Callback(true)
        end
    end, "vulcano sell")
    
    tab_Auto:CreateButton("Sell City Base", function()
    
        main.Tp2(Vector3.new(-173.86717224121094, 223.7541046142578, 1465.10546875))
        wait()
    
        status_auto("Teleporting...")
        main.Tp2(Vector3.new(-174.7919921875, 18.239635467529297, 1606.091796875))
        plat()
        status_cooldown("Countdown", 5)
        unplat()
        main.Tp2(Vector3.new(-246.013916015625, 17.881628036499023, 1616.1649169921875))
        wait(2)
        status_auto("")
    end, "city sell")
    
        tab_Auto:CreateLabel("Auto exit", true)
    
        tab_Auto:CreateButton("Exit Jewelry", function()
        laser1()
        main.path_find(Vector3.new(133.10208129882812, 118.63140869140625,
                                        1294.787109375), 3, 20)
            main.Tp1(Vector3.new(141.88523864746094, 118.63142395019531, 1319.94140625),
                nil, 50)
    end, "jewelry exit")
    
    tab_Auto:CreateButton("Exit Bank", function()
        local a = main.path_find(Vector3.new(38.07012939453125, 18.862625122070312, 916.6630249023438), 3)
        if not a then
            notificar("Erro", 3, 2)
        end
    end)
    
    tab_Auto:CreateToggle('Auto Exit Prison', function(bool)
        AutoEscape = bool
    
        spawn(function()
            while true do 
                pcall(function()
                    while AutoEscape do 
                        repeat wait() until tostring(speaker.Team) == "Prisoner" or AutoEscape == false and getHumanoid().Health > 0
                        if not AutoEscape then 
                            continue
                        end
                        
                        if time_alived < 5 then 
                            for i = 1, 5 - math.floor(time_alived), -1 do 
                                wait(1)
                                notificar("TIME ALIVED ["..i.."]", 2, 5)
                            end
                        end
    
                        if (getRoot().Position - Vector3.new(-2960.52734375, -48.806949615478516, 2419.901123046875)).Magnitude < 50 then 
                            getHumanoid().Health = 0
                            repeat wait() until speaker.CharacterAdded:wait()
                            continue
                        end
    
                        if main.teto(getRoot().Position) then 
                            if getRoot().Position.Y < 3 then 
                                main.path_find(Vector3.new(-1487.6824951171875, -0.4178621768951416, -1865.14697265625))
                                wait(1)
                                main.Tp1(Vector3.new(-1481.05725, -0.458597869, -1865.38452, 4.92290565e-05, -3.77673928e-08, 1, -1.35461775e-09, 1, 3.77674603e-08, -1, -1.35647693e-09, 4.92290565e-05), nil, 100)
                                main.Tp1(Vector3.new(-1476.5675, 2.3979485, -1865.34973, -0.0468534455, -3.49439766e-08, -0.998901784, -3.1616274e-09, 1, -3.48340983e-08, 0.998901784, 1.52605761e-09, -0.0468534455), nil, 100)
                                main.Tp1(Vector3.new(-1468.27319, 8.20495129, -1865.2749, -0.00868753623, 2.75980572e-08, -0.99996227, -9.33462752e-10, 1, 2.76072072e-08, 0.99996227, 1.17326615e-09, -0.00868753623), nil, 100)
                                main.Tp1(Vector3.new(-1461.83411, 12.7130728, -1865.21729, -0.00867221877, -2.64704294e-08, -0.999962389, 8.97336871e-10, 1, -2.64792064e-08, 0.999962389, -1.12693666e-09, -0.00867221877), nil, 100)
                                main.Tp1(Vector3.new(-1456.59546, 16.3807526, -1865.17041, -0.00867221877, 3.47371092e-08, -0.999962389, -1.17749754e-09, 1, 3.47486271e-08, 0.999962389, 1.47880097e-09, -0.00867221877), nil, 100)
                                main.Tp1(Vector3.new(-1454.13965, 18.1145096, -1865.14844, -0.00867221877, -1.08240094e-07, -0.999962389, 3.66910302e-09, 1, -1.08275984e-07, 0.999962389, -4.60795802e-09, -0.00867221877), nil, 100)
                                main.Tp1(Vector3.new(-1447.14233, 19.0321293, -1865.08606, -0.00867221877, 7.37001482e-09, -0.999962389, -2.49827548e-10, 1, 7.37245864e-09, 0.999962389, 3.13753717e-10, -0.00867221877), nil, 100)
                                main.Tp1(Vector3.new(-1445.14246, 19.0321293, -1865.06787, -0.00867221877, 9.89087781e-08, -0.999962389, -3.35280492e-09, 1, 9.89415767e-08, 0.999962389, 4.21072155e-09, -0.00867221877), nil, 100)
                                main.Tp1(Vector3.new(-1445.31311, 18.7980766, -1846.17175, -0.999962449, 1.36101783e-10, 0.00866876543, -2.82184942e-10, 1, -4.82509463e-08, -0.00866876543, -4.82515787e-08, -0.999962449), nil, 100)
                                main.Tp1(Vector3.new(-1445.31311, 25.7009888, -1846.17175, -0.999962449, 1.23415153e-10, 0.00866876543, -2.56936528e-10, 1, -4.38749943e-08, -0.00866876543, -4.38755734e-08, -0.999962449), nil, 100)
                                main.Tp1(Vector3.new(-1418.17651, 25.9422874, -1758.77759, -0.933539391, -7.08888725e-09, 0.358474851, -2.62229936e-08, 1, -4.85147318e-08, -0.358474851, -5.46906946e-08, -0.933539391), nil, 100)
