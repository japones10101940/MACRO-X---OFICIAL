    DropShadow_3.Position = UDim2.new(-0.0031152647, 0, 0.00952380989, 0)
    DropShadow_3.Size = UDim2.new(1, 0, 1, 0)

    UICorner_4.CornerRadius = UDim.new(0, 25)
