    local auto_airdrop_confirm = false
    Farm:CriarBotao(
        'Auto Airdrop',
        function()
            if auto_airdrop_confirm == false then 
                auto_airdrop_confirm = true 
                notificar("Clique novamente", 2, 4)
                notificar("Auto airdrop\nRisco de kick", 2, 4.1)
            else
                local Teste = require(game:GetService('ReplicatedStorage').Module.UI)
                for i, a in next, Teste.CircleAction.Specs do
                    a.Timed = true
                end
                function drop(v)
                    if v.Name == 'Drop' then
                        Tp2(v.Top.Position)
                        saircarro()
                        wait(1)

                        spawn(
                            function()
                                notificar('MACRO X'.. '\nColete o AirDrop!', 2,4)

                                no_clip(true)
                                while getHumanoid(speaker).Health > 0 do
                                    getRoot(speaker).CFrame = v.Top.CFrame * CFrame.new(0, -3.9, 0)
                                    no_clip(false)
                                    local Teste = require(game:GetService('ReplicatedStorage').Module.UI)
                                    for i, a in next, Teste.CircleAction.Specs do
                                        a.Timed = true
                                    end
                                    task.wait()
                                end
                            end
                        )
                    end
                end

                for i, v in pairs(workspace:GetChildren()) do
                    if v.Name == 'Drop' then
                        drop(v)
                    end
                end
            end
            
        end
    )

