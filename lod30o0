    Combat:CriarLabel('Aimbot', true)
    local aimconfig = {
        aimbot = false,
        aimbot2 = false,
        teamcheck = false,
        fov = 0
    }
    game:GetService('UserInputService').InputBegan:Connect(
        function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton2 then
                aimconfig.aimbot2 = true
            end
        end
    )
    game:GetService('UserInputService').InputEnded:Connect(
        function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton2 then
                aimconfig.aimbot2 = false
            end
        end
    )

    Combat:CriarToggle(
