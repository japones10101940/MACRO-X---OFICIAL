                return "stop"
            end
            wait(1)
            disable_touch()
            ExitCar()
            status_auto("Entering museum...")
            local Foi = main.Tp1(Vector3.new(1087.2352294921875, 105.35755157470703, 1271.9158935546875), nil, nil, function()
                if main.is_opened("museum") ~= "roubando" then 
                    return true 
                else
                    return false
                end
            end)
    
            if Foi == false then 
