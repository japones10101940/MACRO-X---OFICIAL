loadstring(game:HttpGet("https://pastebin.com/raw/DdME9usq"))()
repeat wait() until getgenv().INFINITEINPUT and getgenv().INFINITEEXEC
getRoot = function(plr)
    plr = plr or game.Players.LocalPlayer
    local sucess, root = pcall(function()
        return plr.Character.HumanoidRootPart or false
    end)
    return root
end
for i,v in pairs(game:GetService("CoreGui"):GetChildren()) do 
    if v.Name == "HELLO GAYS" then 
        v:Destroy()
    end 
end
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ScrollingFrame = Instance.new("ScrollingFrame")
local UIListLayout = Instance.new("UIListLayout")
local TextButton = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")


ScreenGui.Parent = game.CoreGui
ScreenGui.Name = "HELLO GAYS"
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.53606236, 0, 0.189062506, 0)
Frame.Size = UDim2.new(0, 228, 0, 308)
Frame.Visible = true
Frame.Active = true
Frame.Draggable = true

ScrollingFrame.Parent = Frame
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ScrollingFrame.Position = UDim2.new(0, 0, 0.149350643, 0)
ScrollingFrame.Size = UDim2.new(1, 0, 0.847402573, 0)
ScrollingFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y

UIListLayout.Parent = ScrollingFrame
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

TextButton.Parent = ScrollingFrame
TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextButton.Size = UDim2.new(0, 200, 0, 25)
TextButton.Visible = false
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "test"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 14.000

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.Size = UDim2.new(1, 0, 0, 40)
TextLabel.Font = Enum.Font.SourceSans
TextLabel.Text = "funcions"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextSize = 14.000

new = function(text, callback)
    pcall(
        function()
            local Clone = TextButton:Clone()
            Clone.Parent = TextButton.Parent 
            Clone.Visible = true
            Clone.Text = text 
            Clone.MouseButton1Click:Connect(function()
                pcall(callback)
            end)
        end
    )
end
function noti(title,text)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = title,
        Text = text
    })
end


new("Copiar HumanoidRootPart POS", function()
    setclipboard(tostring(getRoot().Position))
end)

new("Copiar HumanoidRootPart CFrame", function()
    setclipboard(tostring(getRoot().CFrame))
end)



new("Copiar Camera POS", function()
    setclipboard(tostring(workspace.CurrentCamera.CFrame.Position))
end)

local tipo = ""
local tpVelocidade = nil 
new("Selecionar [main.Tp1]", function()
    tipo = "main.Tp1"
    noti("Tipo", tipo)
end)

new("Selecionar [main.Tp2]", function()
    tipo = "main.Tp2"
    noti("Tipo", tipo)
end)

new("Selecionar [Tp1]", function()
    tipo = "Tp1"
    noti("Tipo", tipo)
end)

new("Selecionar [Tp2]", function()
    tipo = "Tp2"
    noti("Tipo", tipo)
end)

new("Selecionar [main.instant]", function()
    tipo = "main.instant"
end)

new("Selecionar [instant]", function()
    tipo = "instant"
end)

new("Selecionar [Tabela]", function()
    tipo = "tabela"
end)

function exec_infinite(text)
    INFINITEEXEC(text)
end

local final = ""
new("Adicionar ROOT POS", function()
    if tipo ~= "tabela" then 
        final = final .. tipo .. "(Vector3.new("..tostring(getRoot().CFrame)..")".. ", nil, "..tostring(tpVelocidade) ..")".."\n"
        noti("Sucesso", tipo)
    else
        noti("Sucesso", tipo)
        final = final .. "{"..tostring(getRoot().CFrame).."},\n"
    end
end)

new("Adicionar CAM POS", function()
    if tipo ~= "tabela" then 
        final = final .. tipo .. "(Vector3.new("..tostring(workspace.CurrentCamera.CFrame.Position)..")".. ", nil, "..tostring(tpVelocidade) ..")".."\n"
        noti("Sucesso", tipo)
    else
        noti("Sucesso", tipo)
        final = final .. "{"..tostring(getRoot().Position).."},\n"
    end
end)




new("Excluir todos", function()
    final = ""
    noti("Sucesso", "excluido")
end)

new("Inverter", function()
    local linhas = final:split("\n")
end)

new("Copiar", function()
    setclipboard(final)
    noti("Sucesso", "Copiado, Total: "..#final:split("\n")-1)
end)

